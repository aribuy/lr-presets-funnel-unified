# Payment Gateway Configuration
# Copy this file to .env and fill in your actual API keys

# Base Configuration
NODE_ENV=production
PORT=3000
BASE_URL=https://presets.agentbar.ai

# Stripe Configuration
STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key_here
STRIPE_SECRET_KEY=sk_live_your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# PayPal Configuration
PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_CLIENT_SECRET=your_paypal_client_secret_here
PAYPAL_MODE=live

# Cryptocurrency (CoinGate)
COINGATE_API_KEY=your_coingate_api_key_here
COINGATE_MODE=live

# Regional Payment Methods

# Indonesia - Midtrans
MIDTRANS_SERVER_KEY=your_midtrans_server_key_here
MIDTRANS_CLIENT_KEY=your_midtrans_client_key_here
MIDTRANS_MODE=production

# India - Razorpay
RAZORPAY_KEY_ID=your_razorpay_key_id_here
RAZORPAY_KEY_SECRET=your_razorpay_key_secret_here

# Malaysia - iPay88
IPAY88_MERCHANT_CODE=your_ipay88_merchant_code_here
IPAY88_MERCHANT_KEY=your_ipay88_merchant_key_here

# Singapore - PayNow
PAYNOW_UEN=your_paynow_uen_here
PAYNOW_MERCHANT_NAME=ARIBUY LLC

# Thailand - 2C2P
2C2P_MERCHANT_ID=your_2c2p_merchant_id_here
2C2P_SECRET_KEY=your_2c2p_secret_key_here

# Philippines - PayMongo
PAYMONGO_PUBLIC_KEY=your_paymongo_public_key_here
PAYMONGO_SECRET_KEY=your_paymongo_secret_key_here

# Brazil - PIX
PIX_KEY=your_pix_key_here
PIX_MERCHANT_NAME=ARIBUY LLC
PIX_MERCHANT_CITY=SAO PAULO

# Mexico - Conekta
CONEKTA_PUBLIC_KEY=your_conekta_public_key_here
CONEKTA_PRIVATE_KEY=your_conekta_private_key_here

# China - Alipay/WeChat Pay
ALIPAY_APP_ID=your_alipay_app_id_here
ALIPAY_PRIVATE_KEY=your_alipay_private_key_here
WECHAT_APP_ID=your_wechat_app_id_here
WECHAT_MCH_ID=your_wechat_mch_id_here
WECHAT_API_KEY=your_wechat_api_key_here

# Bank Transfer Details
BANK_NAME_US=Chase Bank
BANK_ACCOUNT_US=1234567890
BANK_ROUTING_US=021000021
BANK_SWIFT_US=CHASUS33

BANK_NAME_EU=Deutsche Bank
BANK_ACCOUNT_EU=DE89370400440532013000
BANK_SWIFT_EU=DEUTDEFF

BANK_NAME_UK=HSBC UK
BANK_ACCOUNT_UK=12345678
BANK_SORT_CODE_UK=40-02-02
BANK_SWIFT_UK=HBUKGB4B

# Email Configuration (for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password_here

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/presets_funnel

# Analytics
GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
FACEBOOK_PIXEL_ID=YOUR_PIXEL_ID

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here